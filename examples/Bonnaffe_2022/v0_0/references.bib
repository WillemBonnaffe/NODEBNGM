@article{Pontarp2019,
abstract = {Abstract Statistical techniques exist for inferring community assembly processes from community patterns. Habitat filtering, competition, and biogeographical effects have, for example, been inferred from signals in phenotypic and phylogenetic data. The usefulness of current inference techniques is, however, debated as a mechanistic and causal link between process and pattern is often lacking, and evolutionary processes and trophic interactions are ignored. Here, we revisit the current knowledge on community assembly across scales and, in line with several reviews that have outlined challenges associated with current inference techniques, we identify a discrepancy between the current paradigm of eco-evolutionary community assembly and current inference techniques that focus mainly on competition and habitat filtering. We argue that trait-based dynamic eco-evolutionary models in combination with recently developed model fitting and model evaluation techniques can provide avenues for more accurate, reliable, and inclusive inference. To exemplify, we implement a trait-based, spatially explicit eco-evolutionary model and discuss steps of model modification, fitting, and evaluation as an iterative approach enabling inference from diverse data sources. Through a case study on inference of prey and predator niche width in an eco-evolutionary context, we demonstrate how inclusive and mechanistic approaches?eco-evolutionary modelling and Approximate Bayesian Computation (ABC)?can enable inference of assembly processes that have been largely neglected by traditional techniques despite the ubiquity of such processes. Much literature points to the limitations of current inference techniques, but concrete solutions to such limitations are few. Many of the challenges associated with novel inference techniques are, however, already to some extent resolved in other fields and thus ready to be put into action in a more formal way for inferring processes of community assembly from signals in various data sources.},
annote = {Summary
- they build an eco-evo patch-based and trait-based ODE model were dispersion on the phenotypic space is modelled by a mutation kernel.
- They simulate the model from priors over a given time period and compare model outcome to observations. Through an rejection scheme they retain only the best solutions, which gives parameter estimations.

Notes
- the authors state that there is a need for inferring processes from observations, given that not every processes can be accounted for in experiments due to the complexity of natural systems.
- I see that this paper calls for inference technique that rely on a more mechanistic modelling of the dynamics of ecosystem comunities.
- The author states that identifying processes in communities is complicated because there are multiple processes that can produce the same patterns, and often these processes do interplay.
- Authors state that community dynmaics rarely consider both spatial and temporal dynamics, trophic interactions, and evolution.
- they state the approach should be accompanied by model selection => I see that as a potential limit to the framework given the complexity of the processes considered.
- Though their data was simulated on 5000 evolutionary steps, they do not mention how long was the predictive model simulated for. It is hence unclear how that would be overcome in practice.
- I think the statistical scheme is interesting in the context of your NODE paper. You could invoke the ABC rejection principle to justify the recovery of approximate posterior distribution of the parameters. However, you would still need the optimisation step otherwise the fit would be crap.
- On this last note, I think this approach is only suitable for very well specified models in well known systems. Indeed they only simulate from priors, if they infer only two parameters there is a good chance they would sample accidentally a good scenario, this become virtually impossible for higher dimensional models. Hence, you would need at least an optimisation step. But I guess this would work for "simple" model, or for really not greedy "inferences" (i.e. getting at only a few parameters).
- There is alot of shadows surrounding the choice of initial conditions, time period on which to run the model, as this is likely to affect the model output as much if not more than parameter specifications. For instance for the same parameters, if you simulate the model for a short amount of time you would obtain an output close to initial conditions, if you simulate the model for a really long time you will get something different.
- What about model stochasticity? I guess this is accounted for in the ABC rejection scheme.},
author = {Pontarp, Mikael and Br{\"{a}}nnstr{\"{o}}m, {\AA}ke and Petchey, Owen L.},
doi = {10.1111/2041-210X.13129},
file = {:Users/willhem/Library/Application Support/Mendeley Desktop/Downloaded/Pontarp, Br{\"{a}}nnstr{\"{o}}m, Petchey - 2019 - Inferring community assembly processes from macroscopic patterns using dynamic eco-evolutionary.pdf:pdf},
issn = {2041210X},
journal = {Methods in Ecology and Evolution},
keywords = {ABC,biogeography,community assembly,community structure,eco-evolutionary dynamics,ecology,evolution,process inference},
mendeley-tags = {ABC,eco-evolutionary dynamics},
number = {4},
pages = {450--460},
title = {{Inferring community assembly processes from macroscopic patterns using dynamic eco-evolutionary models and Approximate Bayesian Computation (ABC)}},
volume = {10},
year = {2019}
}
@article{Stenseth2002,
abstract = {Across the boreal forest of North America, lynx populations undergo 10- year cycles. Analysis of 21 time series from 1821 to the present demonstrates that these fluctuations are generated by nonlinear processes with regulatory delays. Trophic interactions between lynx and hares cause delayed density- dependent regulation of lynx population growth. The nonlinearity, in contrast, appears to arise from phase dependencies in hunting success by lynx through the cycle. Using a combined approach of empirical, statistical, and mathematical modeling, we highlight how shifts in trophic interactions between the lynx and the hare generate the nonlinear process primarily by shifting functional response curves during the increase and the decrease phases.},
annote = {- prey-predator oscillation in hare and lynx is caused by a shift in intra- and inter-specific interaction strength between the increase and decline phase.
- The shift in intraspecific component can be due to, for the lynx, a break down of the territories as hare decline, and thus intraspe interaction (declines?).
- The shift in intraspe component of the oscillation of the hare is due to a stronger dominance hierachy at high food. Thus during the increase?
- The shift in the interspecfic interaction is caused, for the lynx, by a shift in demography, and hunting strategy.
- The shift in the interspecific interaction for the hare, is caused by a change in resource supplies or habitat selection, and predation pressure.},
