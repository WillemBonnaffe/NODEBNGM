file = {:Users/willhem/Library/Application Support/Mendeley Desktop/Downloaded/Berdahl et al. - 2015 - On the evolutionary interplay between dispersal and local adaptation in heterogeneous environments.pdf:pdf},
issn = {15585646},
journal = {Evolution},
keywords = {Adaptive radiation,Dimorphic populations,Evolutionary branching,Joint evolution,Meta-population,Specialization,dispersal,macroevolution,theoretical},
mendeley-tags = {dispersal,macroevolution,theoretical},
