keywords = {Density-toxicant interaction,Generalized additive modelling,Non-parametric modelling,Stage-specific density dependence,Time-series analysis},
number = {1577},
pages = {2133--2142},
title = {{From patterns to processes and back: Analysing density-dependent responses to an abiotic stressor by statistical and mechanistic modelling}},
volume = {272},
year = {2005}
