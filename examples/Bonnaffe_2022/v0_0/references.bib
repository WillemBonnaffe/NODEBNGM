isbn = {00030147},
issn = {0003-0147},
journal = {The American Naturalist},
keywords = {Guppy,ODE,ODE fit,eco-evolutionary feedback,eco-evolutionary feedbacks,ecological architecture,evolution,fitting,food webs,indirect,indirect effects,modelling,theoretico-empirical},
mendeley-tags = {Guppy,ODE,ODE fit,eco-evolutionary feedbacks,ecological architecture,fitting,indirect effects,modelling,theoretico-empirical},
number = {2},
pages = {167--185},
