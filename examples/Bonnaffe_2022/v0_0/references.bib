journal = {Molecular Ecology},
keywords = {Adaptation,Community structure,Eco-evolutionary feedback loops,Ecologically relevant traits,Ecosystem function,Environmental change,Genome scans,Rapid evolution,dispersal,eco-evolutionary dynamics,eco-evolutionary feedbacks,environmental change,genetic architecture,genomics,genotype,review,spatial dynamics,spatial heterogeneity},
