file = {:Users/willhem/Library/Application Support/Mendeley Desktop/Downloaded/Wu, Fukuhara, Takeda - 2005 - Parameter estimation of an ecological system by a neural network with residual minimization training.pdf:pdf},
issn = {03043800},
journal = {Ecological Modelling},
keywords = {Data assimilation,Delay differential equation,NODE application,Neural network,Parameter estimation,Predator-prey dynamics,Residual minimization training},
