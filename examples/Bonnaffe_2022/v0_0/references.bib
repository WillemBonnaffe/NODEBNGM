keywords = {a rate equal to,average excess,breeder,equation,evolution,fisher,fundamental theorem,mean fi tness at,natural selection increases the,natural selection states that,price equation,s,s fundamental theorem of},
number = {4},
pages = {345--353},
pmid = {28350496},
title = {{Fundamental Theorems of Evolution}},
url = {http://www.journals.uchicago.edu/doi/10.1086/690937},
