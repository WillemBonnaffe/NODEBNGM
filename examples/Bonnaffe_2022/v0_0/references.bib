issn = {00030147},
journal = {American Naturalist},
keywords = {Adaptation,Competition,Damselfly,Density dependence,Eco-evolutionary dynamics,Intra-specific competition,Spatial variation,experimental,phenotype-fitness},
mendeley-tags = {Intra-specific competition,experimental,phenotype-fitness},
