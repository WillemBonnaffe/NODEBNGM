author = {Childs, Dylan Z. and Sheldon, Ben C. and Rees, Mark},
doi = {10.1111/1365-2656.12483},
file = {:Users/willhem/Library/Application Support/Mendeley Desktop/Downloaded/Childs, Sheldon, Rees - 2016 - The evolution of labile traits in sex- and age-structured populations.pdf:pdf},
isbn = {1365-2656},
issn = {13652656},
journal = {Journal of Animal Ecology},
keywords = {IPMs,Integral projection model,Labile trait,Labile traits,Ontogeny,Parus major,Plasticity,Price equation,Quantitative genetics,Quantitative trait,Selection analysis,price equation,quantiative genetics},
mendeley-tags = {IPMs,Labile traits,price equation,quantiative genetics},
number = {2},
pages = {329--342},
pmid = {26899421},
title = {{The evolution of labile traits in sex- and age-structured populations}},
