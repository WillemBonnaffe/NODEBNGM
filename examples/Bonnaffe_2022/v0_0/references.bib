issn = {1365294X},
journal = {Molecular Ecology},
keywords = {cichlid,craniofacial,cryptic genetic variation,fish,genetic architecture,genomics,phenotypic plasticity,plasticity,quantitative trait loci},
mendeley-tags = {fish,genetic architecture,genomics,plasticity},
number = {24},
