@article{Nelson2019,
abstract = {Asymmetric interactions among conspecifics can have diverse effects on population dynamics including stabilization, generation of cycles, and induction of chaotic fluctuations. A difficult challenge, however, is establishing the link between the impact of asymmetric interactions on life history and the consequences for population dynamics. The smaller tea tortrix, Adoxophyes honmai, is a good example. Larval instars differ dramatically in size and have a tendency for cannibalism, which suggests the potential for strong asymmetric interactions among instars. Yet whether these asymmetries have any role in generating the distinct single-generation cycles observed in the field and laboratory is unclear. Here we report on the development of a new experimental approach to characterize the impact of asymmetric interactions on life history that can be directly embedded into stage-structured population models. The experiments use donor-replacement protocols in which focal individuals are challenged to complete their life cycles in competitive environments where the instar and density of the competitors are held constant. The experimentally derived interaction surface contains all the information about stage-specific interactions and provides a straightforward framework for evaluating alternative ways of abstracting the interactions into traditional models of asymmetric competition. Working with the smaller tea tortrix, we found strong evidence of asymmetric interactions and identified critical “tipping points” in the competitive environment that strongly affected survival but not development. We incorporated the experimentally derived interaction surface into a stage-structured population model and found that despite the strong impact that asymmetric interactions have on tea tortrix life history, they do not scale-up to impact the predicted asymptotic population dynamics. Comparing these dynamics with two abstracted models of stage-structured interactions revealed that while the quantitative details of the emergent dynamics depends on the shape of the interaction surface, the qualitative features, such as the emergence of single-generation cycles and rapid synchronization of development among individuals, are pleasingly robust.},
author = {Nelson, W. A. and Joncour, B. and Pak, D. and Bj{\o}rnstad, Ottar N.},
doi = {10.1002/ecy.2558},
file = {:Users/willhem/Library/Application Support/Mendeley Desktop/Downloaded/Nelson et al. - 2019 - Asymmetric interactions and their consequences for vital rates and dynamics the smaller tea tortrix as a model sy.pdf:pdf},
issn = {00129658},
journal = {Ecology},
keywords = {Adoxophyes honmai,asymmetric competition,cannibalism,distributed-delay stage-structured model,intraspecific,life history rates,population dynamics},
number = {2},
pages = {1--15},
pmid = {30411784},
title = {{Asymmetric interactions and their consequences for vital rates and dynamics: the smaller tea tortrix as a model system}},
volume = {100},
year = {2019}
}
@article{Pelletier2009,
abstract = {Evolutionary ecologists and population biologists have recently considered that ecological and evolutionary changes are intimately linked and can occur on the same time-scale. Recent theoretical developments have shown how the feedback between ecological and evolutionary dynamics can be linked, and there are now empirical demonstrations showing that ecological change can lead to rapid evolutionary change. We also have evidence that microevolutionary change can leave an ecological signature. We are at a stage where the integration of ecology and evolution is a necessary step towards major advances in our understanding of the processes that shape and maintain biodiversity. This special feature about 'eco-evolutionary dynamics' brings together biologists from empirical and theoretical backgrounds to bridge the gap between ecology and evolution and provide a series of contributions aimed at quantifying the interactions between these fundamental processes. {\textcopyright} 2009 The Royal Society.},
annote = {Takeaways:
- state that feedbacks are the next challenge, and that they are hard to identify because their complexity of the need for long term studies.

Notes:
- the authors group eco-evolutionary field into three sub-fields: (i) study of effects between phenotypes and demography; (ii) study of interplay between environment and genetic structure of populations; and (iii) importance of microevolutionary processes for community composition and ecosystem processes. I personnally think this classificaiton is somewhat anecdotal and that the papers I have read do not fall clearly into these three distinct modes.
- On the history of the field: the author point at the fact that Darwin and fisher already recognised the interplay between ecology and evolution. Benefited largely from the rising availability in long term time series, and enhanced capacity to detect selection and genetic basis for traits.
- Bi-directional effects of ecology to evolution and back again are feedbacks. At the time the authors wrote their piece, feedbacks seemed to not be inferred yet. Even today when reading Hendry et al. 2017 it seems that it is still not the case...
- The authors state that by definition natural selection should leave a trace on population dynmaics as that operates through deaths and births. What I find confusing is that though natural selection has been evidenced to be at play in the finch system there is no clear signature on the demography of the finches.
- A useful quote here: pelletier et al. 2009 propose a counterquote to Dobzhansky's 'nothing in ecology makes sense except in the light of evolution', twhich goes as follows: 'nothing in evolution makes sense except in the light of ecology'. Which creates a nice chicken and egg problem.
- The author state that feedbacks are not well documented and understood yet because there is a shortage of studies quantifying the evo to eco effects (except in finch, soay sheep, etc.).
- The authors discuss at length how ecology might impact the genetic structure of population, and hence lay the basis for eco-to-evo effects. This reminds me of the importance of considering alternative genetic structures. I overall realise that exploring feedbacks really is about exploring complexity at the phenotypic, demographic, environmental level, opening each box is costly.
- The authors state that long term studies are the way to go to disentangle the complexity of the potential feedbacks. I see this rejoin the statements made in Fussman et al. 2006 regarding the complexity of the systems at play and the need for long term/live monitoring of dynamical systems and anaylsis through mathematical modelling.
- They also point at the fact that feedbacks are possibly only measurable during transient states, where the system is changing. That rejoins the statement of another study... which I need to find the reference of.},
author = {Pelletier, Fanie and Garant, Dany and Hendry, Andrew P.},
doi = {10.1098/rstb.2009.0027},
file = {:Users/willhem/Library/Application Support/Mendeley Desktop/Downloaded/Pelletier, Garant, Hendry - 2009 - Eco-evolutionary dynamics.pdf:pdf},
issn = {14712970},
journal = {Philosophical Transactions of the Royal Society B: Biological Sciences},
keywords = {Contemporary evolution,Eco-evolutionary dynamics,Evolutionary changes,Genotype-by-environment interaction,Natural selection,Population dynamics,eco-evolutionary dynamics,eco-evolutionary feedbacks,phenotypic change,review},
mendeley-tags = {eco-evolutionary dynamics,eco-evolutionary feedbacks,phenotypic change,review},
number = {1523},
pages = {1483--1489},
pmid = {19414463},
title = {{Eco-evolutionary dynamics}},
volume = {364},
year = {2009}
}
