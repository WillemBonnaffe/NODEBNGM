journal = {Nature Ecology and Evolution},
keywords = {ecological networks,meta barcoding,metacommunity,methods,spatial heterogeneity,transcriptomic},
mendeley-tags = {ecological networks,meta barcoding,metacommunity,methods,spatial heterogeneity,transcriptomic},
number = {2},
pages = {1--11},
pmid = {28812622},
publisher = {Macmillan Publishers Limited},
title = {{Species-rich networks and eco-evolutionary synthesis at the metacommunity level}},
