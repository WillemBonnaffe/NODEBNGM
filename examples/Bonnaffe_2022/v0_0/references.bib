file = {:Users/willhem/Library/Application Support/Mendeley Desktop/Downloaded/Moser, Roesti, Berner - 2012 - Repeated Lake-Stream Divergence in Stickleback Life History within a Central European Lake Basin.pdf:pdf},
issn = {19326203},
journal = {PLoS ONE},
keywords = {Sticklebacks},
mendeley-tags = {Sticklebacks},
number = {12},
title = {{Repeated Lake-Stream Divergence in Stickleback Life History within a Central European Lake Basin}},
volume = {7},
year = {2012}
}
