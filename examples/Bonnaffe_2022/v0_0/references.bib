journal = {Ecology},
keywords = {Conservation biology,Demography,Extinction risk,Homogeneous populations,Population viability analysis,Stochastic demography,Variance reduction effect},
number = {7},
pages = {1928--1934},
title = {{Demographic stochasticity and the variance reduction effect}},
volume = {83},
year = {2002}
}
