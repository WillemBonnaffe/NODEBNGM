issn = {10902716},
journal = {Journal of Computational Physics},
keywords = {Bayesian,Bayesian neural networks,Convolutional encoder–decoder networks,Deep learning,PDE,Porous media flows,Uncertainty quantification,uncertainity quantification},
mendeley-tags = {Bayesian,PDE,uncertainity quantification},
pages = {415--447},
publisher = {Elsevier Inc.},
title = {{Bayesian deep convolutional encoder–decoder networks for surrogate modeling and uncertainty quantification}},
