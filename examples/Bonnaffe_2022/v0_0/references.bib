@article{Cotto2017,
abstract = {Withstanding extinction while facing rapid climate change depends on a species' ability to track its ecological niche or to evolve a new one. Current methods that predict climate-driven species' range shifts use ecological modelling without eco-evolutionary dynamics. Here we present an eco-evolutionary forecasting framework that combines niche modelling with individual-based demographic and genetic simulations. Applying our approach to four endemic perennial plant species of the Austrian Alps, we show that accounting for eco-evolutionary dynamics when predicting species' responses to climate change is crucial. Perennial species persist in unsuitable habitats longer than predicted by niche modelling, causing delayed range losses; however, their evolutionary responses are constrained because long-lived adults produce increasingly maladapted offspring. Decreasing population size due to maladaptation occurs faster than the contraction of the species range, especially for the most abundant species. Monitoring of species' local abundance rather than their range may likely better inform on species' extinction risks under climate change.},
annote = {1. Introduction
- Classic niche models assume niche are constant. 
- Evolutionary change of the niche can however be fast and alter short term response to environmental perturbation (e.g. climate change).

2. Methods
- Simple system of bioclimatic enveloppe of one plant species accross a spatial range.
- IBM model with explicit genetic architecture (10 genes for three traits). Mutation generate new allelic effects.
- Fitness/pop growth given by multivariate gaussian on traits, optimum set by the environment.

4. Discussion
- Evolutionnary rescue is very unlikely in this system.
- But evolution is at work and changes the state of local populations => delays the range loss. => should be less bad than expected.},
author = {Cotto, Olivier and Wessely, Johannes and Georges, Damien and Klonner, G{\"{u}}nther and Schmid, Max and Dullinger, Stefan and Thuiller, Wilfried and Guillaume, Fr{\'{e}}d{\'{e}}ric},
doi = {10.1038/ncomms15399},
file = {:Users/willhem/Library/Application Support/Mendeley Desktop/Downloaded/Cotto et al. - 2017 - A dynamic eco-evolutionary model predicts slow response of alpine plants to climate warming.pdf:pdf},
issn = {20411723},
journal = {Nature Communications},
keywords = {climate change,eco-evolutionary dynamics},
mendeley-tags = {climate change,eco-evolutionary dynamics},
number = {May},
pages = {1--9},
pmid = {28474676},
title = {{A dynamic eco-evolutionary model predicts slow response of alpine plants to climate warming}},
volume = {8},
year = {2017}
}
