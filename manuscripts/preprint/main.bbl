    \verb 10.3389/fevo.2021.740362
    \endverb
    \field{issn}{2296701X}
    \field{title}{Geometric Complexity and the Information-Theoretic Comparison
  of Functional-Response Models}
    \field{volume}{9}
    \field{journaltitle}{Frontiers in Ecology and Evolution}
    \field{month}{11}
    \field{year}{2021}
  \endentry

